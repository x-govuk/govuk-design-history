{
  "name": "govuk-design-history",
  "version": "1.0.0",
  "description": "A design history for your GOV.UK service",
  "homepage": "https://x-govuk.github.io/govuk-design-history",
  "bugs": {
    "url": "https://github.com/x-govuk/govuk-design-history/issues"
  },
